{
  "name": "overcome-gravity-api",
  "version": "0.0.1",
  "description": "Backend api for the Overcome Gravity app.",
  "author": "Michael Penrod",
  "license": "MIT",
  "scripts": {
    "postinstall": "tsc",
    "start": "node dist/main.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch",
    "tslint": "npm run tslint:src && npm run tslint:test",
    "tslint:src": "tslint src/**/*.ts",
    "tslint:test": "tslint test/**/*.ts"
  },
  "dependencies": {
    "@nestjs/common": "^6.0.5",
    "@nestjs/core": "^6.0.5",
    "@nestjs/mongoose": "^6.0.0",
    "@nestjs/platform-express": "^6.0.5",
    "body-parser": "^1.18.3",
    "joi": "^14.3.1",
    "lodash.clonedeep": "^4.5.0",
    "mongoose": "^5.4.20",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "rollbar": "^2.5.4",
    "rxjs": "^6.4.0",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.0.5",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.11",
    "@types/joi": "^14.3.2",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/mongoose": "^5.3.24",
    "@types/morgan": "^1.7.35",
    "@types/node": "^11.12.2",
    "@types/supertest": "^2.0.7",
    "jest": "^24.5.0",
    "rimraf": "^2.6.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.1",
    "ts-loader": "^5.3.3",
    "ts-node": "^8.0.3",
    "tsconfig-paths": "^3.8.0",
    "tslint": "5.14.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".(spec|test).ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
